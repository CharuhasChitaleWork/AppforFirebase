<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="test.example.charuhaschitale.appforfirebase">

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="test.example.charuhaschitale.appforfirebase.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="test.example.charuhaschitale.appforfirebase.screen11" >
        <intent-filter>
            <action android:name="android.intent.action.VIEW"/>
            <category android:name="android.intent.category.DEFAULT"/>
            <category android:name="android.intent.category.BROWSABLE"/>
            <data android:host="http://localhost/demo/demohome.html" android:scheme="http"/>
            <data android:host="http://localhost/demo/demohome.html" android:scheme="https"/>
        </intent-filter>
        </activity>
        <activity android:name="test.example.charuhaschitale.appforfirebase.screen12" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:host="https://play.google.com/store/apps/details?id=test.example.charuhaschitale.appforfirebase" android:scheme="http"/>
                <data android:host="https://play.google.com/store/apps/details?id=test.example.charuhaschitale.appforfirebase" android:scheme="https"/>
            </intent-filter>
        </activity>
        <activity android:name="test.example.charuhaschitale.appforfirebase.screen21" />
        <activity android:name="test.example.charuhaschitale.appforfirebase.screen22"/>

    </application>


</manifest>